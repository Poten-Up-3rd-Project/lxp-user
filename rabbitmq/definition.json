{
  "rabbit_version": "3.13.0",
  "exchanges": [
    {
      "name": "auth.events",
      "vhost": "auth",
      "type": "topic",
      "durable": true,
      "auto_delete": false
    },
    {
      "name": "user.events",
      "vhost": "user",
      "type": "topic",
      "durable": true,
      "auto_delete": false
    }
  ],
  "queues": [
    {
      "name": "auth-events.user-registered",
      "vhost": "auth",
      "durable": true,
      "auto_delete": false
    },
    {
      "name": "user-events.user-creation-failed",
      "vhost": "user",
      "durable": true,
      "auto_delete": false
    }
  ],
  "bindings": [
    {
      "source": "auth.events",
      "vhost": "auth",
      "destination": "auth-events.user-registered",
      "destination_type": "queue",
      "routing_key": "UserRegisteredIntegrationEvent"
    },
    {
      "source": "user.events",
      "vhost": "user",
      "destination": "user-events.user-creation-failed",
      "destination_type": "queue",
      "routing_key": "UserCreationFailedIntegrationEvent"
    }
  ]
}
